<view><view><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><swiper-item a:for="{{imgUrls}}" a:for-item="item" a:for-index="index" a:key="index"><image class="slide-image" src="{{item.img}}" width="355" height="150" mode="aspectFit"></image></swiper-item></swiper></view><view class="header"><view><image class="img-item" src="{{userInfo.avatar}}" data-event-opts="{{[['tap',[['toLogin']]]]}}" onTap="__e"></image></view><view><text class="title">{{userInfo.nickname}}</text><text class="description">生活于市井，游离于从林</text></view></view></view>